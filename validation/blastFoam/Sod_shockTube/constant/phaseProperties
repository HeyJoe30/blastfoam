type basic;
thermoType
{
    transport const;
    thermo eConst;
    equationOfState idealGas;
};
equationOfState
{
    gamma       1.4;
    omega 0.4;
    rho0 1000.0;
    R1 4.1;
    R2 1.2;
    A 1.0;
    B 5.0;
}

specie
{
    molWeight    11640.3;
}
