FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U.orig;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    air_to_plate
    {
        type            movingWallVelocity;
        value           $internalField;
    }
    plateEnd
    {
        type            slip;
    }
    left
    {
        type            zeroGradient;
    }
    right
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    wedge
    {
        type            wedge;
    }
}

