FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      quadratureProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

fieldMomentInversion basicVelocityFieldMomentInversion;


basicVelocityMomentInversion
{
    type monoKinetic;
    basicQuadrature
    {
        univariateMomentInversion   Gauss;
    }
}

momentAdvection
{
    velocityMomentAdvection   Vikas;
}

moments
(
    (0 0)
    (0 1)
//     (0 2)
//     (0 3)
//     (0 4)

    (1 0)
    (1 1)
//     (1 2)
//     (1 3)
//     (1 4)

    (2 0)
    (3 0)

);

nodes
(
    (0 0)
    (1 0)
