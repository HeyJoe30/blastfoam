FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/solid";
    object      solidProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solidModel linearElastic;

linearElasticCoeffs
{
    nCorrectors 1000;
    solutionTolerance 1e-6;
}


