FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidMechanicProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

planeStress off;
E
{
    type    uniform;
    value   1.86e+11;
}
nu
{
    type    uniform;
    value   0.3;
}

mechanical
(
    steel
    {
        type            neoHookeanElasticMisesPlastic;
        rho             rho [ 1 -3 0 0 0 0 0 ] 7800;
        E               E [ 1 -1 -2 0 0 0 0 ] 1.86e+11;
        nu              nu [ 0 0 0 0 0 0 0 ] 0.3;
        values
        (
            ( 0          241316504.79999998 )
            ( 0.0035     275790291.2 )
            ( 0.0083     301645631 )
            ( 0.0133     318882524.2 )
            ( 0.0182     344737864 )
            ( 0.0281     361974757.2 )
            ( 0.0380     379211650.4 )
            ( 1          1482372815.2 )
        );
        outOfBounds     clamp;
        smoothPressure  yes;
        updateBEbarConsistent yes;
    }
);

